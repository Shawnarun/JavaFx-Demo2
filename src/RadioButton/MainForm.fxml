<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="RadioButton.MainFormController">
   <children>
      <Label layoutX="24.0" layoutY="26.0" text="Radion button" textFill="#462fb9">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
      <RadioButton fx:id="rBtnFemale" layoutX="24.0" layoutY="106.0" mnemonicParsing="false" text="Female">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
         <toggleGroup>
            <ToggleGroup fx:id="gender" />
         </toggleGroup>
      </RadioButton>
      <RadioButton fx:id="rBtnMale" layoutX="226.0" layoutY="106.0" mnemonicParsing="false" text="Male" toggleGroup="$gender">
         <font>
            <Font name="System Bold" size="29.0" />
         </font>
      </RadioButton>
      <Button layoutX="24.0" layoutY="321.0" mnemonicParsing="false" onAction="#printDataOnAction" prefHeight="39.0" prefWidth="171.0" style="-fx-background-color: blue;" text="Print Data" textFill="#fffefe">
         <font>
            <Font size="21.0" />
         </font>
      </Button>
      <Separator layoutX="26.0" layoutY="77.0" prefHeight="5.0" prefWidth="557.0" />
   </children>
</AnchorPane>
